public class divisaoreais {
    public static void main(String[] args) {

        double numero1 = 1.2;
        double numero2 = 2.0;

        double resultadoDivisao = numero1 / numero2;

        System.out.println("A divisão de " + numero1 + " por " + numero2 + " é: " + resultadoDivisao);
    }
}
